{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.36/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.json",
  "resize_delta": 50,
  "animation": {
    "enabled": true,
    "duration": 400,
    "fps": 90,
    "style": {
      "movement": "EaseOutBack",
      "transparency": "Linear"
    }
  },
  "border": true,
  "border_colours": {
    "unfocused": "#D8DEE9",
    "stack": "#D8DEE9",
    "monocle": "#D8DEE9",
    "single": "#D8DEE9",
    "floating": "#D8DEE9"
  },
  "border_implementation": "Komorebi",
  "border_offset": -1,
  "border_style": "Square",
  "border_width": 3,
  "cross_monitor_move_behaviour": "Insert",
  "default_container_padding": 9,
  "default_workspace_padding": 9,
  "float_override": false,
  "global_work_area_offset": {
    "left": 0,
    "top": 0,
    "bottom": 0,
    "right": 0
  },
  "mouse_follows_focus": true,
  "stackbar": {
    "tabs": {
      "font_size": 0,
      "background": "#ffffff",
      "font_family": "Segoe UI",
      "unfocused_text": "#ffffff",
      "width": 300,
      "focused_text": "#ffffff"
    },
    "height": 40,
    "label": "Process",
    "mode": "OnStack"
  },
  "transparency": false,
  "transparency_alpha": 180,
  "unmanaged_window_operation_behaviour": "Op",
  "window_container_behaviour": "Create",
  "window_hiding_behaviour": "Minimize",
  "monitors": [
    {
      "workspaces": [
        {
          "name": "I",
          "layout": "BSP"
        },
        {
          "name": "II",
          "layout": "VerticalStack"
        },
        {
          "name": "III",
          "layout": "BSP"
        },
        {
          "name": "IV",
          "layout": "BSP"
        },
        {
          "name": "V",
          "layout": "BSP"
        }
      ]
    },
    {
      "workspaces": [
        {
          "name": "1",
          "layout": "BSP"
        },
        {
          "name": "2",
          "layout": "RightMainVerticalStack"
        },
        {
          "name": "3",
          "layout": "UltrawideVerticalStack"
        },
        {
          "name": "4",
          "layout": "Rows"
        },
        {
          "name": "5",
          "layout": "Rows"
        }
      ]
    }
  ]
}
